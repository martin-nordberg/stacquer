{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true,
    "skipLibCheck": true,
    "paths": {
      "$shared/*": [
        "../../shared/stacquer-shared/src/*"
      ]
    }
  },
  "include": [
    "src",
    "vitest"
  ],
  "exclude": [
    "node_modules"
  ]
}
